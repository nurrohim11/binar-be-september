                "$ref": "#/components/schemas/Login"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type":"object",
                  "properties":{
                    "status":{
                      "type": "integer",
                      "format": "int64",
                      "example": 200
                    },
                    "message":{
                      "type": "string",
                      "example": "Successful"
                    }
                  }
                }
              }
            }
          },
          "400":{
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiResponse"
                }
              }
            }
          }
        }
      }
    },

